# environment variables
RUN_ENV="production"

# simpledb configurations
AWS_PROFILE="sdb_prod"
SDB_DOMAIN="ART_prod_build"
AWS_DEFAULT_REGION=us-east-1

# mysql
MYSQL_HOST=mysql.art-build-dev.svc
MYSQL_DB_NAME=build
MYSQL_USER=admin
MYSQL_CONNECTION_PORT=3306

# github endpoints
GITHUB_RATELIMIT_ENDPOINT = https://api.github.com/rate_limit

# github raw content group.yml url
GITHUB_RAW_CONTENT_URL = https://raw.githubusercontent.com/openshift/ocp-build-data/{}/group.yml

# errata advisory api
ERRATA_ADVISORY_ENDPOINT = https://errata.devel.redhat.com/api/v1/erratum/{}
